function crearFichero($file){
@unlink($file);
$total = 0;
$fp = fopen($file, 'w');
qQuery("select cd_gs_user,login,pass from gs_user order by cd_gs_user");
while( $r=qRow() ){
$r[1] = trim($r[1]);
$r[2] = trim($r[2]);
if( empty($r[1]) || empty($r[2]) ){
continue;
}
fputs($fp, "{$r[0]}|{$r[1]}|{$r[2]}\n");
$total++;
}
fclose($fp);
return $total;
}