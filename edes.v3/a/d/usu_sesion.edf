[P]OtrasVariables
global $_Form;
$Pasar = false;
echo '<table border=0 cellspacing=0 cellpadding=0>';
$source = file_get_contents("../_datos/config/session.ini");
$rem = array(
array( "/"."*" ,  "*"."/"),
array("<!"."--", "--".">")
);
for($n=0; $n<count($rem); $n++){
$ini = mb_strpos($source, $rem[$n][0]);
$end = mb_strpos($source, $rem[$n][1], $ini);
while( $ini!==false && $end!==false && $end>$ini ){
$source = mb_substr($source,0,$ini).mb_substr($source, $end+mb_strlen($rem[$n][1]));
$ini = mb_strpos($source, $rem[$n][0], $ini+1);
$end = mb_strpos($source, $rem[$n][1], $ini);
}
}
$dimSess = array();
$dim = explode("\n", $source);
for($n=0; $n<count($dim); $n++){
$txt = trim($dim[$n]);
if( $txt=="" ) continue;
list($txt) = explode(REM, $txt);
$txt = trim($txt);
if( $txt=="" ) continue;
$dim[$n] = $txt;
if( preg_match('/(\$_SESS\[)("|\')(\w){1,30}("|\')\]\s{0,9}\=/u', $txt,  $coincide,  PREG_OFFSET_CAPTURE) ){
if( preg_match('/("|\')(\w){1,30}("|\')/u', $coincide[0][0],  $coincide,  PREG_OFFSET_CAPTURE) ){
$dimSess[mb_substr($coincide[0][0], 1, -1)] = 1;
}
}
}
foreach($dimSess as $key=>$value){
echo "<tr><td style='text-align:right'>{$key}<td>";
switch( getType($value) ){
case 'double':
eAddField("| {$key} | -, | T | 6,3 |  | M | | |", str_replace(".",",",$value));
break;
case 'integer':
eAddField("| {$key} | - | T | 6 |  | M | | |", str_replace(".",",",$value));
break;
default:
eAddField("| {$key} | #X | T | 100 | 225 | M | | |", $value);
}
}
echo '</table>';
[PHPIni]M
global $_pField;
$Pasar = false;
eMessage("ERROR: proces no implementado", "HSE");
foreach($ _ ENV[SESS] as $key=>$value) if( getType($value)!="array" && getType($value)!="boolean" && getType($value)!="NULL" ){
if( $Pasar && $key!='_User' && $key!='_Tree' && $key!='_Node' ){
switch( getType($value) ){
case 'double':
$_pField[$key] = explode("|", "| {$key} | -, | T | 6,3 |  | M | | |");
break;
case 'integer':
$_pField[$key] = explode("|", "| {$key} | - | T | 6 |  | M | | |");
break;
default:
$_pField[$key] = explode("|", "| {$key} | #X | T | 100 | 225 | M | | |");
}
}
if( $key=='_Tree' ) $Pasar = true;
}