[PHPStart]*
$n = 1;
$nomScript = "";
foreach($_GET as $nomScript=>$v) if( ++$n>2 ) break;
$nomScript .= ".gdf";
$ok = false;
$dim = file(eScript($nomScript));
for($n=0; $n<count($dim); $n++){
$dim[$n] = trim($dim[$n]);
$txt = mb_strtoupper($dim[$n]);
if( mb_substr($txt,0,8)=="[MYDATA]" ){
$ok = true;
list($nomField, $nomVar) = explode("=", trim(explode("/", mb_substr($dim[$n],8))[0]));
$nomField = trim($nomField);
$nomVar = trim($nomVar);
$_POST[$nomField] = SESS::${$nomVar};
break;
}
}
if( !$ok ) eEnd();
$OriFichero = $nomScript;
$_FileDF = $nomScript;
$Fichero = eScript($nomScript);
$FicheroD = $nomScript;
$_SERVER["REQUEST_METHOD"] = 'POST';
if( $_Mode=="cR" ) $_NOBUTTON = 1;
$_STOP = 1;
$_POST["_MD5"] = 1;
[]
#Include(*) {$nomScript}